<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.techinvest.pcplrealestate.fragment.Projectdetails" >

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:background="#ffffff"
        android:dividerPadding="12dip"
        android:orientation="horizontal"
        android:showDividers="middle"
        android:weightSum="2" >

        <Button
            android:id="@+id/button1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/developed" />

        <View
            android:layout_width="1dp"
            android:layout_height="fill_parent"
            android:layout_marginBottom="7dp"
            android:layout_marginTop="7dp"
            android:background="#90909090" />

        <Button
            android:id="@+id/button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/underconstruction" />
    </LinearLayout>

    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/multiAutoCompleteTextView1"
        android:layout_marginTop="30dp"
        android:layout_toLeftOf="@+id/textView6"
        android:ems="10"
         android:background="@drawable/edittext_medium"
        android:text="AutoCompleteTextView" >

        <requestFocus />
    </AutoCompleteTextView>

    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/autoCompleteTextView3"
        android:layout_marginTop="30dp"
        android:layout_toStartOf="@+id/textView6"
        android:ems="10"
         android:background="@drawable/edittext_medium"
        android:text="AutoCompleteTextView" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/autoCompleteTextView2"
        android:layout_alignLeft="@+id/textView8"
        android:text="@string/architech"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/autoCompleteTextView3"
        android:layout_alignLeft="@+id/textView6"
        android:layout_marginTop="30dp"
        android:text="@string/ShareCertificationno"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/editText1"
        android:layout_toLeftOf="@+id/autoCompleteTextView1"
        android:text="@string/societyname"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <EditText
        android:id="@+id/editText1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/autoCompleteTextView4"
        android:layout_below="@+id/autoCompleteTextView4"
        android:layout_marginTop="30dp"
        android:background="@drawable/edittextstyle2"
        android:ems="10" />

    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/autoCompleteTextView3"
        android:layout_alignLeft="@+id/multiAutoCompleteTextView1"
        android:ems="10"
         android:background="@drawable/edittext_medium"
        android:text="AutoCompleteTextView" />

    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/autoCompleteTextView4"
        android:layout_alignStart="@+id/autoCompleteTextView5"
        android:ems="10"
         android:background="@drawable/edittext_medium"
        android:text="AutoCompleteTextView" />

    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/autoCompleteTextView4"
        android:layout_below="@+id/autoCompleteTextView2"
        android:layout_marginTop="30dp"
        android:ems="10"
         android:background="@drawable/edittext_medium"
        android:text="AutoCompleteTextView" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/linearLayout1"
        android:layout_marginLeft="114dp"
        android:layout_marginTop="30dp"
        android:text="@string/buildername"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView1"
        android:layout_marginLeft="48dp"
        android:layout_marginStart="44dp"
        android:layout_toEndOf="@+id/textView1"
        android:layout_toRightOf="@+id/textView1"
        android:ems="10"
        android:background="@drawable/edittext_medium"
        android:text="AutoCompleteTextView" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/autoCompleteTextView4"
        android:layout_alignBottom="@+id/autoCompleteTextView4"
        android:layout_alignLeft="@+id/textView8"
        android:text="@string/sharedistinctiveno"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignLeft="@+id/autoCompleteTextView6"
        android:ems="10"
         android:background="@drawable/edittext_medium"
        android:text="AutoCompleteTextView" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/autoCompleteTextView2"
        android:layout_alignLeft="@+id/textView1"
        android:layout_toLeftOf="@+id/autoCompleteTextView2"
        android:text="@string/planno"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/autoCompleteTextView4"
        android:layout_alignLeft="@+id/textView2"
        android:text="@string/ccno"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/editText1"
        android:layout_alignLeft="@+id/textView3"
        android:text="@string/societyno"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/autoCompleteTextView1"
        android:layout_marginLeft="44dp"
        android:layout_toRightOf="@+id/autoCompleteTextView1"
        android:text="@string/Unitdetails"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <AutoCompleteTextView
        android:id="@+id/multiAutoCompleteTextView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView6"
        android:layout_marginLeft="21dp"
        android:layout_toRightOf="@+id/textView8"
        android:ems="10"
         android:background="@drawable/edittext_medium"
        android:text="AutoCompleteTextView" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/autoCompleteTextView7"
        android:background="@drawable/button_background"
        android:textColor="#FFFFFF"
        android:layout_margin="10dp"
        android:text="@string/save" />

</RelativeLayout>