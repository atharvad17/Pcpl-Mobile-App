<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
     xmlns:tools="http://schemas.android.com/tools"
     android:layout_width="fill_parent"
     android:layout_height="fill_parent" 
     android:fillViewport="true"
     tools:context="com.techinvest.pcplrealestate.Requestdetails" >

  <RelativeLayout
      android:id="@+id/relativelayout"
      android:layout_width="match_parent"
      android:layout_height="match_parent" >

      <TextView
          android:id="@+id/textView3"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignLeft="@+id/textView2"
          android:layout_below="@+id/textView2"
          android:layout_marginTop="30dp"
          android:text="@string/ownername"
          android:textAppearance="?android:attr/textAppearanceMedium" />

      <TextView
          android:id="@+id/textView4"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignLeft="@+id/textView3"
          android:layout_below="@+id/textView3"
          android:layout_marginTop="30dp"
          android:text="@string/Address"
          android:textAppearance="?android:attr/textAppearanceMedium" />

      <EditText
          android:id="@+id/editText2"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_above="@+id/textView3"
          android:layout_alignLeft="@+id/editText1"
          android:layout_marginTop="30dp"
          android:background="@drawable/edittextstyle2"
          android:ems="10" />

      <EditText
          android:id="@+id/editText3"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignBottom="@+id/textView3"
          android:layout_alignLeft="@+id/editText2"
           android:background="@drawable/edittextstyle2"
          android:layout_marginTop="30dp"
          android:ems="10" />

      <EditText
          android:id="@+id/editText4"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignBottom="@+id/textView4"
          android:layout_alignLeft="@+id/editText3"
          android:layout_marginTop="30dp"
           android:background="@drawable/edittextstyle2"
          android:ems="10" />

      <TextView
          android:id="@+id/textView10"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_above="@+id/textView3"
          android:layout_alignLeft="@+id/textView9"
          android:layout_marginTop="30dp"
          android:text="@string/nameofboard"
          android:textAppearance="?android:attr/textAppearanceMedium" />

      <EditText
          android:id="@+id/editText8"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_above="@+id/textView3"
          android:layout_alignLeft="@+id/editText7"
           android:background="@drawable/edittextstyle2"
          android:layout_marginTop="30dp"
          android:ems="10" />

      <TextView
          android:id="@+id/textView1"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignParentLeft="true"
          android:layout_alignParentTop="true"
          android:layout_marginLeft="63dp"
          android:layout_marginTop="42dp"
          android:text="@string/Enginner"
          android:textAppearance="?android:attr/textAppearanceMedium" />

      <TextView
          android:id="@+id/textView2"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignLeft="@+id/textView1"
          android:layout_below="@+id/editText1"
          android:layout_marginTop="30dp"
          android:text="@string/CustomerName"
          android:textAppearance="?android:attr/textAppearanceMedium" />

      <EditText
          android:id="@+id/editText1"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignBottom="@+id/textView1"
          android:layout_marginStart="40dp"
          android:layout_toEndOf="@+id/textView2"
          android:background="@drawable/edittextstyle2"
          android:ems="10" />

      <TextView
          android:id="@+id/textView9"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_above="@+id/textView10"
          android:layout_marginStart="81dp"
          android:layout_toEndOf="@+id/editText1"
          android:text="@string/Bankname"
          android:textAppearance="?android:attr/textAppearanceMedium" />

      <EditText
          android:id="@+id/editText5"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignBottom="@+id/textView5"
          android:layout_alignStart="@+id/editText4"
           android:background="@drawable/edittextstyle2"
          android:layout_marginTop="30dp"
          android:ems="10" />

      <TextView
          android:id="@+id/textView11"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignBottom="@+id/editText3"
          android:layout_alignStart="@+id/textView10"
          android:text="@string/Address2"
          android:textAppearance="?android:attr/textAppearanceMedium" />

      <EditText
          android:id="@+id/editText9"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_above="@+id/textView4"
          android:layout_alignStart="@+id/editText8"
           android:background="@drawable/edittextstyle2"
          android:ems="10" />

      <TextView
          android:id="@+id/textView12"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignBottom="@+id/editText4"
          android:layout_alignStart="@+id/textView11"
          android:text="@string/Location"
          android:textAppearance="?android:attr/textAppearanceMedium" />

      <EditText
          android:id="@+id/editText10"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignBaseline="@+id/textView12"
          android:layout_alignBottom="@+id/textView12"
          android:layout_alignStart="@+id/editText9"
           android:background="@drawable/edittextstyle2"
          android:ems="10" >

          <requestFocus />
      </EditText>

      <TextView
          android:id="@+id/textView5"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignStart="@+id/textView4"
          android:layout_below="@+id/editText10"
          android:layout_marginTop="30dp"
          android:text="@string/Address3"
          android:textAppearance="?android:attr/textAppearanceMedium" />

      <TextView
          android:id="@+id/textView6"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignStart="@+id/textView5"
          android:layout_below="@+id/textView5"
          android:layout_marginTop="29dp"
          android:text="@string/Area"
          android:textAppearance="?android:attr/textAppearanceMedium" />

      <AutoCompleteTextView
          android:id="@+id/requestautoCompleteTextview1"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignBottom="@+id/textView6"
          android:layout_alignStart="@+id/editText5"
           android:background="@drawable/edittext_medium"
          android:ems="10"
        
          android:hint="Area"/>

      <TextView
          android:id="@+id/textView7"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignStart="@+id/textView6"
          android:layout_below="@+id/textView6"
          android:layout_marginTop="32dp"
          android:text="@string/personmet"
          android:textAppearance="?android:attr/textAppearanceMedium" />

      <EditText
          android:id="@+id/editText6"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignBottom="@+id/textView7"
          android:layout_alignStart="@+id/requestautoCompleteTextview1"
           android:background="@drawable/edittextstyle2"
           android:hint="Person Met"
          android:ems="10" />

      <TextView
          android:id="@+id/textView8"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignStart="@+id/textView7"
          android:layout_below="@+id/textView7"
          android:layout_marginTop="30dp"
          android:text="@string/Occupant"
          android:textAppearance="?android:attr/textAppearanceMedium" />

      <AutoCompleteTextView
          android:id="@+id/requestautoCompleteTextView4"
          android:layout_width="220dp"
          android:layout_height="25dp"
          android:layout_alignBottom="@+id/textView8"
           android:background="@drawable/edittext_medium"
          android:layout_toStartOf="@+id/textView9" />

      <TextView
          android:id="@+id/textView13"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignStart="@+id/textView8"
          android:layout_below="@+id/textView8"
          android:layout_marginTop="30dp"
          android:text="Medium Text"
          android:textAppearance="?android:attr/textAppearanceMedium" />

      <AutoCompleteTextView
          android:id="@+id/requestautoCompleteTextView2"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignBottom="@+id/textView13"
           android:background="@drawable/edittext_medium"
          android:layout_alignLeft="@+id/requestautoCompleteTextView4"
          android:ems="10"
          android:hint="Self" />

      <TextView
          android:id="@+id/textView14"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_above="@+id/textView6"
          android:layout_alignStart="@+id/textView12"
          android:text="@string/PINCODE"
          android:textAppearance="?android:attr/textAppearanceMedium" />

      <TextView
          android:id="@+id/textView15"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_above="@+id/textView7"
          android:layout_alignStart="@+id/textView14"
          android:text="@string/propertytype"
          android:textAppearance="?android:attr/textAppearanceMedium" />

     <AutoCompleteTextView
          android:id="@+id/requestautoCompleteTextView5"
          android:layout_width="220dp"
          android:layout_height="25dp"
          android:layout_alignBottom="@+id/textView15"
           android:background="@drawable/edittext_medium"
           android:hint="Property Type"
          android:layout_alignStart="@+id/requestautoCompleteTextView3" />

      <AutoCompleteTextView
          android:id="@+id/requestautoCompleteTextView3"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignBottom="@+id/textView14"
          android:layout_alignStart="@+id/editText10"
           android:background="@drawable/edittext_medium"
          android:ems="10"
          android:hint="Pincode" />

      <Button
          android:id="@+id/button1"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignEnd="@+id/requestautoCompleteTextView5"
          android:layout_alignTop="@+id/textView13"
          android:layout_marginBottom="10dp"
          android:background="@drawable/button_background"
          android:textColor="#ffffff"
          android:text="@string/save" />

      <EditText
          android:id="@+id/editText7"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_above="@+id/editText8"
          android:layout_marginStart="47dp"
          android:layout_toEndOf="@+id/textView10"
          android:background="@drawable/edittextstyle2"
          android:ems="10" />

  </RelativeLayout>

</ScrollView>